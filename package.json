{
  "name": "error-codes-generator",
  "displayName": "Error codes generator",
  "description": "`Error codes generator` is a powerful VS Code extension designed to streamline your development workflow by providing tools for generating, formatting, and managing error codes and nomenclatures directly within your editor. This extension helps maintain consistency and clarity in your codebase, especially in large projects.",
  "version": "0.1.1",
  "author": "Joel287g",
  "publisher": "Joel287g",
  "repository": {
    "type": "git",
    "url": "https://github.com/Joel287g/error-codes-generator.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/Joel287g/error-codes-generator#readme",
  "bugs": {
    "url": "https://github.com/Joel287g/error-codes-generator/issues"
  },
  "keywords": [
    "error",
    "codes",
    "generator",
    "formatter",
    "nomenclature"
  ],
  "icon": "src/assets/icon.png",
  "preview": true,
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Formatters",
    "Linters"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "web-coding-solution.generateErrorCounter",
        "title": "Generate error counter"
      },
      {
        "command": "web-coding-solution.formatErrorCounter",
        "title": "Format error counter"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "json",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "ts-loader": "^9.5.4",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
